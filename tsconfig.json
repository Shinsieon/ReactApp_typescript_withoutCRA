{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES5",
    "module": "esnext",
    "jsx": "react",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "DOM.Iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": false,
    "forceConsistentCasingInFileNames": false,
    "moduleResolution": "node"
  },
  "include": ["src"]
}
